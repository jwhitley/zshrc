# zchain_enable_new_agent=true

python_bin_dir=/c/Python25
python_script_dir=$python_bin_dir/Scripts

perl_bin_dir=/c/Perl/bin

ruby_bin_dir=${${(M)commands:#*ruby.exe}:h}
emacs_bin_dir=/programs/emacs-23.1/bin
path_prepend=( $emacs_bin_dir $ruby_bin_dir $python_bin_dir $python_script_dir $perl_bin_dir )

